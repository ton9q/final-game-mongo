{
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "browser": true,
    "jquery": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "max-len": ["error", { "code": 120, "tabWidth": 2 }],
    "arrow-parens": [2, "as-needed"]
  }
}